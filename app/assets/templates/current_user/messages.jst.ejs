<h1> Hello from messages!</h1>
<ul>
<% conversations.each(function(conversation){ %>
  <a href="#/messages/<%= conversation.id %>"><li><p>Conversation between <%= conversation.get("user_one").username %> and <%= conversation.get("user_two").username %> </p>
      <p><%= conversation.get("latest_reply").user.username %>: <%= conversation.get("latest_reply").body %> </p>
  </li></a>
<% }) %>
</ul>
