<h1> Matches </h1>
<ul class='users group'>
  <% users.each(function(user) { %>
    <a href="#/users/<%= user.id %>">
      <li class="user-index-block">
        <img class="user-picture" src="http://placehold.it/280x150">
        <h3><%= user.escape('username') %></h3>
        <p><%= user.escape('language') %></p>
        <p> <%= current_user.assess_compatibility(user) %>% Match</p>
      </li>
    </a>
  <% }) %>
</ul>
